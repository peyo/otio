generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Emotion {
  id        Int      @id @default(autoincrement())
  type      String   // "Happy", "Sad", "Anxious", "Angry", "Neutral"
  intensity Int      // 0 for Neutral, 1-3 for others
  createdAt DateTime @default(now())
}
